  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "es2016"],
    "module": "esnext",
    "rootDir": "ts",
    "outDir": "static", 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}
